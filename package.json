  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "main": "lib/index.js",
    "build": "babel index.js App.js src src/assets --out-dir lib && npm run copy-assets && npm run copy-app-json",
    "copy-assets": "cpx \"src/assets/**/*\" lib/assets",
    "copy-app-json": "cpx app.json lib",
    "prepublishOnly": "npm run build",
    "bundle:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "android:uat": "setEnv.bat uat && react-native run-android --mode=uatDebug",
    "android:prod": "setEnv.bat prod && react-native run-android --mode=prodDebug",
    "android:dev": "setEnv.bat development && react-native run-android --mode=devDebug",
    "assemble-uat": "setEnv.bat uat && cd android && gradlew assembleUatRelease",
    "assemble-prod": "setEnv.bat prod && cd android && gradlew assembleProdRelease",
    "assemble-dev": "setEnv.bat development && cd android && gradlew assembleDevRelease"
  },
